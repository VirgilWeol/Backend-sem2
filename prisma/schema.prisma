generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model faculty {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar(255)
}

model students {
  id      Int     @id @default(autoincrement())
  name    String  @db.VarChar(255)
  address String? @db.VarChar(255)
}
